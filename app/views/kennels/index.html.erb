<p id="notice"><%= notice %></p>

<h1>Kennels</h1>

<table>
  <thead>
    <tr>
      <th>Kennel type</th>
      <th>Material cost</th>
      <th>Labor cost</th>
      <th>Price</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kennels.each do |kennel| %>
      <tr>
        <td><%= kennel.kennel_type %></td>
        <td><%= kennel.material_cost %></td>
        <td><%= kennel.labor_cost %></td>
        <td><%= kennel.price %></td>
        <td><%= kennel.customer_id %></td>
        <td><%= link_to 'Show', kennel %></td>
        <td><%= link_to 'Edit', edit_kennel_path(kennel) %></td>
        <td><%= link_to 'Destroy', kennel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kennel', new_kennel_path %>
